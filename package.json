{
  "name": "koa-simple-static",
  "description": "Simple caching static file server for Koa 2.",
  "version": "0.7.2",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src/index.js --out-file lib/index.js",
    "clean": "if [ -d lib ] ; then rm -r lib ; fi && mkdir lib",
    "prebuild": "npm run test && npm run clean",
    "test": "npm run test:lint && npm run test:flow && npm run test:tape",
    "test:flow": "flow && flow stop",
    "test:lint": "eslint -c .eslintrc.json src",
    "test:tape": "babel-tape-runner test.js | tap-spec"
  },
  "homepage": "https://github.com/zacanger/koa-simple-static#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/koa-simple-static.git"
  },
  "bugs": {
    "url": "https://github.com/zacanger/koa-simple-static/issues"
  },
  "keywords": [
    "cache",
    "file",
    "gzip",
    "koa",
    "koa2",
    "middleware",
    "sendfile",
    "server",
    "static"
  ],
  "dependencies": {
    "compressible": "2.0.11",
    "fs-readdir-recursive": "1.0.0",
    "mime-types": "2.1.16",
    "mz": "2.6.0",
    "zeelib": "2.0.6"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-flow-comments": "6.22.0",
    "babel-preset-latest-minimal": "1.1.2",
    "babel-tape-runner": "2.0.1",
    "bluebird": "3.5.0",
    "eslint": "4.4.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-flowtype-errors": "3.3.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "flow-bin": "0.52.0",
    "flow-coverage-report": "0.3.0",
    "istanbul": "0.4.5",
    "koa": "2.3.0",
    "supertest": "3.0.0",
    "tap-spec": "4.1.1",
    "tape": "4.8.0"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "typings": "koa-simple-static.d.ts"
}
